{
    // Required
    "manifest_version": 3,
    "name": "chrome-ex-yt",
    "version": "0.0.1",

    // Recommended
    "action": {
        "default_icon": {
            "16": "img/icon16.png",
            "32": "img/icon32.png",
            "48": "img/icon48.png",
            "64": "img/icon64.png"
        },
        "default_popup": "index.html"
    },
    "description": "chrome extension for youtube tools",
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png"
    },

    "permissions": [
        "tabs",
        "storage"
    ],

    // Optional
    "author": "https://github.com/Doesystem/chrome-ex-yt"
}
